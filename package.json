{
  "name": "wazzup-test-task",
  "description": "Notes app",
  "author": "Vladimir Gerasimov <freelancervip@gmail.com",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --watch --runInBand --detectOpenHandles",
    "start": "npm run dev",
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint --fix src",
    "migrate": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "rollback": "knex --knexfile src/db/knexfile.ts migrate:rollback",
    "seed": "knex --knexfile src/db/knexfile.ts seed:run",
    "initdb": "npm run migrate && npm run seed"
  },
  "engines": {
    "node": "~14.15.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/ioredis": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/nanoid": "^2.1.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "mock-knex": "^0.4.9",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^9.2.1",
    "supertest": "^6.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/express-promise-router": "^3.0.0",
    "@types/jest": "^26.0.19",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-promise-router": "^4.0.1",
    "express-validator": "^6.7.0",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "knex-migrate": "^1.7.4",
    "knex-paginate": "^1.2.3",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.18",
    "pg": "^8.5.1",
    "pg-cursor": "^2.5.1",
    "ts-jest": "^26.4.4"
  }
}
